    "import numpy as np\n",
    "from scipy import stats\n",
    "\n",
    "def rsquared(x_values, y_values):\n",
    "    correlation_matrix = np.corrcoef(x_values, y_values)\n",
    "    correlation_xy = correlation_matrix[0,1]\n",
    "    r_squared = correlation_xy**2\n",
    "\n",
    "    return r_squared"
   ]
